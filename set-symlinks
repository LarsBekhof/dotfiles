#!/bin/zsh

mkdir $HOME/.config

ln -sf $HOME/.vimrc $HOME
ln -sf $HOME/.jshintrc $HOME
ln -sf $HOME/.i3 $HOME
ln -sf $HOME/.i3blocks.conf $HOME
ln -sf $HOME/.Xdefaults $HOME
ln -sf $HOME/dunst $HOME/.config
ln -sf $HOME/.zshrc $HOME
ln -sf $HOME/.zshrc /root
ln -sf $HOME/.aliases $HOME
ln -sf $HOME/.aliases /root
